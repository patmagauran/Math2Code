    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch texParser",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "testTexNew"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Launch math2code",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "test"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/translator/latexToCode.test.tsx",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}